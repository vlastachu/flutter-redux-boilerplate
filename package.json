{
	"name": "flutter-redux-boilerplate",
	"displayName": "Flutter redux boilerplate",
	"description": "boilerplate generator",
	"version": "0.0.1",
  "license": "MIT",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:flutterReduxBoilerplate.generate"
	],
	"keywords": [
		"flutter",
		"redux",
		"dart",
		"boilerplate"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "flutterReduxBoilerplate.generate",
				"title": "Generate Flutter Redux Boilerplate"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.36.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2"
	},
	"dependencies": {
		"@types/q": "^1.0.0",
		"q": "^1.4.1",
		"@types/mkdirp": "^0.3.29",
		"mkdirp": "^0.5.1"
	}
}
